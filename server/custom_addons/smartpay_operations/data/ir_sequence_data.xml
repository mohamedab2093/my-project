<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for helpdesk.request -->
        <record id="seq_helpdesk_request" model="ir.sequence">
            <field name="name">Helpdesk Request</field>
            <field name="code">helpdesk.request</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="suffix">%(day)s%(h24)s</field>
            <field name="padding">5</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <!--<field name="use_date_range" eval="True"/>-->
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
