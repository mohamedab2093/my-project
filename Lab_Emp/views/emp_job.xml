<?xml version="1.0" encoding="utf-8"?>
    <odoo>
        <record id="view_empjob_form" model="ir.ui.view">
            <field name ="name">empjob.view.form</field>
            <field name ="model">empjob</field>
            <field name="arch" type="xml">
                <form>
                <sheet>
                    <group>
                         <field name="name"/>

                    </group>
                </sheet>
                </form>



            </field>
        </record>

         <record id="view_empjob_tree" model="ir.ui.view">
            <field name ="name">empjob.view.tree</field>
            <field name ="model">empjob</field>
             <field name="arch" type="xml">
                 <tree>
                  <field name="name"/>

                 </tree>
            </field>
        </record>




    </odoo>

