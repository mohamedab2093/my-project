<?xml version="1.0" encoding="utf-8"?>
    <odoo>
        <record id="view_labreson_form" model="ir.ui.view">
            <field name ="name">labreson.view.form</field>
            <field name ="model">labreson</field>
            <field name="arch" type="xml">
                <form>
                <sheet>
                    <group>
                         <field name="name"/>

                    </group>
                </sheet>
                </form>



            </field>
        </record>

         <record id="view_labreson_tree" model="ir.ui.view">
            <field name ="name">labreson.view.tree</field>
            <field name ="model">labreson</field>
             <field name="arch" type="xml">
                 <tree>
                  <field name="name"/>

                 </tree>
            </field>
        </record>




    </odoo>

